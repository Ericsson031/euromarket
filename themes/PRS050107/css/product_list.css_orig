ul#in_grid {
    list-style-type: none;
    overflow: hidden;
}
ul#in_grid li {
     background-color: #F2F2F2;
    border: 1px solid #E3E3E3;
    border-radius: 3px 3px 3px 3px;
    float: left;
    margin: 8px 6px;
    padding: 5px;
    position: relative;
    text-align: center;
    width: 210px;

}
ul#in_grid li:hover	{ background-color:#E5E5E5;}
ul#in_grid li div.center_block {
   
}
ul#in_grid li div.top_block {
   
}
ul#in_grid a.product_img_link {
  display: block;
    position: relative;
    text-align: center;
}
ul#in_grid a.product_img_link img { margin-top:5px; }

#product_list li h3 a {
    font-weight:bold;
	
}
#product_list li h3 a:hover {text-decoration:none; color:#5F9D00;}
#product_list li .content_price .price {
    font-size: 15px;
    margin: 0px!important;
}
.price, .price-shipping, .price-wrapping {
    color: #000;
    font-size: 14px;
    font-weight: bold;
    margin-bottom: 6px;
    white-space: nowrap;
}
ul#in_grid li .exclusive {
    margin: 3px auto;
}

ul#in_grid li p.compare {
     clear: both;
    font-size: 11px;
    margin: 5px 0 0;
    text-align:center;
	padding:0;
}
ul#in_list li p.compare{  font-size: 11px;
    margin: 8px 0 0;
    text-align:left;
	padding:0;}
ul#in_list li {
    border-bottom: 1px solid #F2F2F2;
    overflow: auto;
    padding: 8px;
}
ul#in_list li .left_block {
    background-color: #F2F2F2;
    float: left;
    padding: 7px;
	border-radius:5px;
	width:212px;
}
ul#in_list li .left_block:hover	{ background-color:#E5E5E5;}
ul#in_list a.product_img_link {
    background-color: #FFFFFF;
    border: 1px solid #E3E3E3;
    border-radius: 5px 5px 5px 5px;
    display: block;
    float: left;
    padding: 3px;
    position: relative;
}

ul#in_list li .center_block {
    padding:0;
	margin-left:8px;
}
ul#in_grid li.last_item{margin-right:0;}
#product_list .compare input{border:none;}

.prod_img		{ background-color: #FFFFFF;
    border: 1px solid #E3E3E3;
    border-radius: 5px 5px 5px 5px;
    display: block;
    position: relative;
    text-align: center;}
	
.bottom_block		{ float:left; width:100%;}
.bottom_block_left	{ float:left; margin-top:5px; margin-left:5px;}	

#list-view .center_block h3	{ margin:0;}

