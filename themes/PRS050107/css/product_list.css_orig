	 background: url("../img/new-sale-2.png") no-repeat scroll 0 0 transparent;
    color: #FFFFFF;
    display: block;
    font-size: 0;
    height: 37px;
    left: -14px;
    padding: 1px 4px;
    position: absolute;
    text-align: center;
